                <Param name="stopwordList" value="organization_names_stop_words.txt"/>
                <Param name="separator" value="\s+"/>
              </TransformInput>
              <Param name="min" value="1"/>
              <Param name="max" value="2147483647"/>
            </TransformInput>
          </Compare>
          <Compare id="jaroWinkler1" required="true" weight="1" metric="jaroWinkler" threshold="0.3" indexing="true">
            <TransformInput id="regexReplace1" function="regexReplace">
              <TransformInput id="lowerCase" function="lowerCase">
